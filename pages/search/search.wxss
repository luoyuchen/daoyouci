/*search.wxss*/
.searchBtn{
    display:inline-block;
    width:4rem;
    height:60rpx;
    line-height:60rpx;
    background-color:#333;color:#fff;
    margin-left: 3rpx;
    border-radius: none;
}
.searchInput{
    width:13rem;
    height: 58rpx;
    display: inline-block;
    border:1px solid #333;
    padding:0 5px;
    text-align: left;
    color:#333;
    font-size:15px;
}
.search{
    margin:20px auto;
    text-align: center;
}
.history{
    color:#333;
    font-size:15px;
    margin: 10px 20px;
    text-align:left;
}
.history text{
    display:inline-block;
    padding:5px;
    border:1px solid #333;
    margin:0 5px;
}
.template{
    color: #666
}
image{width: 100%;height: 270px}